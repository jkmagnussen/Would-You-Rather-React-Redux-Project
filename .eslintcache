/div>)\n    } else if (Object.keys(this.state.user).length > 0)\n    {\n      return <Routes />;\n    } else {\n      return <video\n        className=\"pitch\"\n        controls\n        autostart\n        autoPlay\n        muted\n        src={video}\n        type=\"video/mp4\" />\n    }\n  }\n\n  logout = () => {\n    this.setState({\n      user: {}\n    })\n    localStorage.setItem(\"userObject\", undefined)\n    window.localStorage.clear();\n    this.props.history.push(\"/\")\n  }\n  \n  render()\n  {\n    console.log(this.state.user)\n    const content = <Routes />;\n\n    return (\n      <div className=\"container\">\n          <Fragment>\n          <Header logout={this.logout} userProfile={this.state.user} login={this.toggleLogin} signUp={this.toggleRegister}/>\n\n            {this.conditionalRender()}\n            <Footer />\n          </Fragment>\n      </div>\n    );\n  }\n}\n\nexport default withRouter(App);\n","/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/reducers/index.js",[],["118","119"],"/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/middleware/index.js",[],"/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/components/Routes.js",["120"],"import React from \"react\";\nimport { Switch, Route } from \"react-router-dom\";\nimport PropTypes from \"prop-types\";\n\nimport Leaderboard from \"./Leaderboard\";\nimport CreateQuestion from \"./CreateQuestion\";\nimport Dashboard from \"./Dashboard\";\nimport ErrorPage from \"./ErrorPage\";\nimport QuestionInfo from \"./QuestionInfo\";\n\nfunction Routes(props) {\n  return (\n    <div className=\"container\">\n      <Switch>\n        <Route path=\"/dashboard\" exact component={Dashboard} />\n\n        <Route path=\"/leaderboard\" exact component={Leaderboard} />\n        <Route path=\"/add\" exact component={CreateQuestion} />\n        