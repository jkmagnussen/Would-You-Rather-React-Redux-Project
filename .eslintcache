     const self = this;\n   }\n  \n  setUser = (user) => {\n    localStorage.setItem(\"userObject\", JSON.stringify(user))\n  }\n\n  conditionalRender = () =>{\n    if (this.state.showLogin === true & Object.keys(this.state.user).length == 0){\n      return (\n        <div>\n          <Login setUser={this.setUser} />\n          <video\n            className=\"pitch\"\n            style={{ marginTop: 25 }}\n            controls\n            autostart\n            autoPlay\n            muted\n            src={video} type=\"video/mp4\" />\n        </div>\n      )\n    }\n    else if (this.state.showSignUp === true){ \n      return (\n        <div>\n        <Register />\n          <video\n            className=\"pitch\"\n            style={{ marginTop: 25 }}\n            controls\n            autostart\n            autoPlay\n            muted\n            src={video}\n            type=\"video/mp4\" />\n      </div>)\n    }else{\n      return <video\n        className=\"pitch\"\n        controls\n        autostart\n        autoPlay\n        muted\n        src={video}\n        type=\"video/mp4\" />\n    }\n  }\n  \n  render(){\n    const content = <Routes />;\n\n    return (\n      <div className=\"container\">\n        <Router>\n          <Fragment>\n            <Header login={this.toggleLogin} signUp={this.toggleRegister}/>\n\n            {this.conditionalRender()}\n            \n            {content}\n            <Footer />\n          </Fragment>\n        </Router>\n      </div>\n    );\n  }\n}\n\nexport default App;\n","/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/reducers/index.js",[],["114","115"],"/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/middleware/index.js",[],"/Users/josephmagnussen/Documents/Programming/Udacity/React Developer Nanodegree/Redux/Would You Rather/src/components/Routes.js",["116"],"import React from \"react\";\nimport { Swi